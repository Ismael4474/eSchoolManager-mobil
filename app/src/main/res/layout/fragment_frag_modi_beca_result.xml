<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".modelo.beca.FragModiBecaResult">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="-23dp">


        <TextView
            android:id="@+id/text8"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="16dp"
            android:text="Codi beca:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text1"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:text="Adjudicant:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text8" />

        <TextView
            android:id="@+id/text2"
            android:layout_width="96dp"
            android:layout_height="25dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:text="Import Inicial:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text1" />

        <TextView
            android:id="@+id/text7"
            android:layout_width="112dp"
            android:layout_height="27dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="12dp"
            android:text="Import Restant:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text2" />

        <EditText
            android:id="@+id/editTexAltaBecatImport"
            android:layout_width="73dp"
            android:layout_height="27dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text=""
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toEndOf="@+id/text2"
            app:layout_constraintTop_toBottomOf="@+id/editTexModiBecaNom" />


        <EditText
            android:id="@+id/editTexAltaBecatImportRestant"
            android:layout_width="73dp"
            android:layout_height="27dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="12dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text=""
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toEndOf="@+id/text7"
            app:layout_constraintTop_toBottomOf="@+id/editTexAltaBecatImport" />

        <TextView
            android:id="@+id/text9"
            android:layout_width="28dp"
            android:layout_height="25dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="€"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.025"
            app:layout_constraintStart_toEndOf="@+id/editTexAltaBecatImport"
            app:layout_constraintTop_toBottomOf="@+id/editTexModiBecaNom" />

        <TextView
            android:id="@+id/text6"
            android:layout_width="28dp"
            android:layout_height="25dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:text="€"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.025"
            app:layout_constraintStart_toEndOf="@+id/editTexAltaBecatImport"
            app:layout_constraintTop_toBottomOf="@+id/text9" />

        <TextView
            android:id="@+id/text5"
            android:layout_width="86dp"
            android:layout_height="24dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:text="Servei: "
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text4" />

        <EditText
            android:id="@+id/editTexModiBecaCodi"
            android:layout_width="101dp"
            android:layout_height="29dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="16dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="number"
            android:text=""
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toEndOf="@+id/text1"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTexModiBecaNom"
            android:layout_width="204dp"
            android:layout_height="29dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="8dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="textPersonName"
            android:text=""
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toEndOf="@+id/text1"
            app:layout_constraintTop_toBottomOf="@+id/editTexModiBecaCodi" />

        <Spinner
            android:id="@+id/spinnerModiBecaEstudiant"
            android:layout_width="173dp"
            android:layout_height="28dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:background="#E8E0E0"
            android:dropDownSelector="@color/white"
            android:scrollbarThumbHorizontal="@color/black"
            android:textColor="@color/black"
            app:layout_constraintStart_toEndOf="@+id/text4"
            app:layout_constraintTop_toBottomOf="@+id/editTexAltaBecatImportRestant" />

        <TextView
            android:id="@+id/text4"
            android:layout_width="84dp"
            android:layout_height="26dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:text="Estudiant: "
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text7" />


        <Spinner
            android:id="@+id/spinnerModiBecaServei"
            android:layout_width="172dp"
            android:layout_height="29dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="4dp"
            android:background="#E8E0E0"
            android:dropDownSelector="@color/white"
            android:scrollbarThumbHorizontal="@color/black"
            android:textColor="@color/black"
            app:layout_constraintStart_toEndOf="@+id/text5"
            app:layout_constraintTop_toBottomOf="@+id/spinnerModiBecaEstudiant" />

        <CheckBox
            android:id="@+id/checkBoxModiBecaFinal"
            android:layout_width="125dp"
            android:layout_height="48dp"
            android:buttonTint="@color/black"
            android:text="Finalitzada"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spinnerModiBecaServei" />

        <Button
            android:id="@+id/buttonModiBecaAceptar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:gravity="center"
            android:text="Aceptar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/checkBoxModiBecaFinal" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>