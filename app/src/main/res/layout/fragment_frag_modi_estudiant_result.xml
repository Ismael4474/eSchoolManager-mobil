<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".modelo.estudiant.FragModiEstudiantResult">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="-23dp">


        <TextView
            android:id="@+id/textView"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="32dp"
            android:text="Codi :"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:text="Cognom:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView1" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:text="DNI:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="119dp"
            android:layout_height="29dp"
            android:layout_marginStart="44dp"
            android:text="Data Naixement:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="119dp"
            android:layout_height="29dp"
            android:layout_marginStart="44dp"
            android:text="AdreÃ§a:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="117dp"
            android:layout_height="29dp"
            android:layout_marginStart="44dp"
            android:text="Telefon:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="116dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:text="Email:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <EditText
            android:id="@+id/editTextModiEstudiantCodi"
            android:layout_width="92dp"
            android:layout_height="26dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="144dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="number"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.924"
            app:layout_constraintStart_toEndOf="@+id/textView"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextModiEstudiantNom"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:layout_marginTop="4dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.538"
            app:layout_constraintStart_toEndOf="@+id/textView1"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantCodi" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:text="Nom:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <EditText
            android:id="@+id/editTextModiEstudiantCognom"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:layout_marginTop="4dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.538"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantNom" />


        <EditText
            android:id="@+id/editTextModiEstudiantDni"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="32dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.83"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantCognom" />

        <EditText
            android:id="@+id/editTextModiEstudiantDataNa"
            android:layout_width="113dp"
            android:layout_height="27dp"
            android:layout_marginTop="4dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="date"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.059"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantDni" />

        <EditText
            android:id="@+id/editTextModiEstudiantAdreca"
            android:layout_width="223dp"
            android:layout_height="27dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantDataNa" />

        <EditText
            android:id="@+id/editTextModiEstudiantTelefon"
            android:layout_width="101dp"
            android:layout_height="28dp"
            android:layout_marginTop="4dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="number"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.067"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantAdreca" />

        <EditText
            android:id="@+id/editTextModiEstudiantEmail"
            android:layout_width="144dp"
            android:layout_height="28dp"
            android:layout_marginTop="4dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="textEmailAddress"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.102"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantTelefon" />

        <ImageButton
            android:id="@+id/imageButtonDateModiEstudiant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:drawableTint="@color/white"
            android:src="@drawable/ic_baseline_calendar_month_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.067"
            app:layout_constraintStart_toEndOf="@+id/editTextModiEstudiantDataNa"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantDni" />

        <Button
            android:id="@+id/buttonModiEstudiantAceptar"
            android:layout_width="115dp"
            android:layout_height="46dp"
            android:gravity="center"
            android:text="Aceptar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/checkBoxRegistrat" />

        <DatePicker
            android:id="@+id/dataPickerModiEstudiant"
            android:layout_width="350dp"
            android:layout_height="450dp"
            android:background="@color/black"
            android:backgroundTint="#D6D6D6"
            android:calendarTextColor="@color/black"
            android:headerBackground="@color/purple_500"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.737"
            app:layout_constraintStart_toStartOf="parent" />

        <CheckBox
            android:id="@+id/checkBoxRegistrat"
            android:layout_width="97dp"
            android:layout_height="33dp"
            android:buttonTint="@color/black"
            android:text="Registrat"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextModiEstudiantEmail" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>