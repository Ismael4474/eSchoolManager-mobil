<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".modelo.estudiant.FragAltaEstudiant">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="-23dp">>

        <TextView
            android:id="@+id/textViewTitulo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/alfa_slab_one"
            android:shadowColor="#151313"
            android:text="Alta Nou Estudiant"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="36dp"
            android:text="Nom:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewTitulo" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="12dp"
            android:text="Cognom:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView1" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="12dp"
            android:text="DNI:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="129dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="12dp"
            android:text="Data Naixement:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="129dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="12dp"
            android:text="AdreÃ§a:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="129dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="12dp"
            android:text="Telefon:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="129dp"
            android:layout_height="30dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="12dp"
            android:text="Email:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <EditText
            android:id="@+id/editTextAltaEstudiantNom"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:layout_marginTop="36dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.637"
            app:layout_constraintStart_toEndOf="@+id/textView1"
            app:layout_constraintTop_toBottomOf="@+id/textViewTitulo" />

        <EditText
            android:id="@+id/editTextAltaEstudiantCognom"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:layout_marginTop="16dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.637"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantNom" />

        <EditText
            android:id="@+id/editTextAltaEstudiantDni"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="32dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantCognom" />

        <EditText
            android:id="@+id/editTextAltaEstudiantDataNa"
            android:layout_width="113dp"
            android:layout_height="27dp"
            android:layout_marginTop="16dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="date"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.066"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantDni" />


        <EditText
            android:id="@+id/editTextAltaEstudiantAdreca"
            android:layout_width="223dp"
            android:layout_height="27dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantDataNa" />

        <EditText
            android:id="@+id/editTextAltaEstudiantTelefon"
            android:layout_width="151dp"
            android:layout_height="28dp"
            android:layout_marginTop="16dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="number"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.103"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantAdreca" />

        <EditText
            android:id="@+id/editTextAltaEstudiantEmail"
            android:layout_width="151dp"
            android:layout_height="28dp"
            android:layout_marginTop="12dp"
            android:background="#E8E0E0"
            android:ems="10"
            android:inputType="textEmailAddress"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.103"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantTelefon" />

        <ImageButton
            android:id="@+id/imageButtonDateAltaEstudiant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@color/white"
            android:drawableTint="@color/white"
            android:src="@drawable/ic_baseline_calendar_month_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.152"
            app:layout_constraintStart_toEndOf="@+id/editTextAltaEstudiantDataNa"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantDni" />

        <Button
            android:id="@+id/buttonAltaEstudiantAceptar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:gravity="center"
            android:text="Aceptar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextAltaEstudiantEmail" />

        <DatePicker
            android:id="@+id/dataPickerAltaEstudiant"
            android:layout_width="350dp"
            android:layout_height="450dp"
            android:layout_marginTop="12dp"
            android:background="@color/black"
            android:backgroundTint="#D6D6D6"
            android:calendarTextColor="@color/black"
            android:headerBackground="@color/purple_500"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.737"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textViewTitulo"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>